/*
Transfer an integer variable n(n > 0) into hexadecimal format.
*/
char I2C(int k){
    if (k >= 10){
        return k - 10 + 'A';
    }
    return k + '0';
}

string HoTex(int n)
{
    string ret = "";
    for(int i = 15; n > 0 ; n = n >> 4){
        ret = I2C(i&n) + ret;
    }
    return ret;
}
